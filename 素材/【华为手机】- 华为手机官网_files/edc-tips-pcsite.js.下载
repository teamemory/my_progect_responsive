$(".browsehappy").remove()
$(document).ready(function(){
	$('html,body').animate({ scrollTop: 0});
	var beginDate=new Date(Date.UTC(2016, 9, 14, 16));
	var endDate=new Date(Date.UTC(2016, 9, 14, 20));
	var now=new Date();
	var edContent = edc['en']
	if(edc[SITE_CODE])	edContent = edc[SITE_CODE]
	if(SITE_CODE == "mx")	edContent = edc["lm-es"]
	if(now >=beginDate && now<=endDate && typeof(isucm) !="undefined"){
		$('body').prepend("<div class='edt-tips' style='position:absolute;width:100%;text-align:center;background: #990000;color: #FFF'>"+
		"<div style='width:98%;margin: 0 auto;max-width:1210px'>" + 
		"<p style='padding:0;position:relative;z-index:10000'><span style='width:98%;float:left;padding:15px 0;'></span><img style='position: absolute;top: 10px;' class='tips-close close_edc' src='http://consumer-res.huaweistatic.com/fr/ucmc/groups/public/documents/webasset/img_con_it_home_close.png'></p></div></div>")
		$('.edt-tips').find('p > span').append(edContent)
		$('.edt-tips').show()
		$('#cbg-main-nav-wrapper').css('top','50px')
		$('#cbg-main').css('padding-top', '117px')
		$('.close_edc').click(function(e){
			$('.edt-tips').remove()
			$('#cbg-main-nav-wrapper').css('top','0')
			$('#cbg-main').css('padding-top', '67px')
		})
		$(window).on('scroll',function(){
			var _c = $(window).scrollTop()
			if(_c>0){
				$('.edt-tips').hide()
				$('#cbg-main-nav-wrapper').css('top','0')
				$('#cbg-main').css('padding-top', '67px')
			}else{
				if($('.edt-tips').length > 0){
					$('.edt-tips').show()
					$('#cbg-main-nav-wrapper').css('top','50px')
					$('#cbg-main').css('padding-top', '117px')
				}else{
					$('#cbg-main-nav-wrapper').css('top','0')
					$('#cbg-main').css('padding-top', '67px')
				}
			}
		})	
	}
})